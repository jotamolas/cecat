{% extends 'base.html.twig' %}

{% block body %}
    <h1>Notarytypes list</h1>

    <table>
        <thead>
            <tr>
                <th>Id</th>
                <th>Description</th>
                <th>Actions</th>
            </tr>
        </thead>
        <tbody>
        {% for notaryType in notaryTypes %}
            <tr>
                <td><a href="{{ path('notarytype_show', { 'id': notaryType.id }) }}">{{ notaryType.id }}</a></td>
                <td>{{ notaryType.description }}</td>
                <td>
                    <ul>
                        <li>
                            <a href="{{ path('notarytype_show', { 'id': notaryType.id }) }}">show</a>
                        </li>
                        <li>
                            <a href="{{ path('notarytype_edit', { 'id': notaryType.id }) }}">edit</a>
                        </li>
                    </ul>
                </td>
            </tr>
        {% endfor %}
        </tbody>
    </table>

    <ul>
        <li>
            <a href="{{ path('notarytype_new') }}">Create a new notaryType</a>
        </li>
    </ul>
{% endblock %}
