<div class="table-responsive">
    <table class="table table-striped" style="font-size: 14px">
        <thead>
            <tr>
                <th>#</th>
                <th>Notario</th>
                <th>Fecha</th>
                <th>Creditos</th>
            </tr>
        </thead>
        <tbody>
            {% for q in queries %}
                <tr>
                    <td>{{ q.id }}</td>
                    <td>{{ q.transaction.notary.lastname }}, {{ q.transaction.notary.firstname }}</td>
                    <td>{{ q.transaction.createdAt | date('d/m/Y') }}</td>
                    <td>{{ q.transaction.ammount }}</td>
                </tr>
            {% endfor %}
        </tbody>
    </table>
</div>